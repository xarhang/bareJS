{
  "name": "barejs",
  "version": "0.1.45",
  "author": "xarhang",
  "description": "High-performance JIT-specialized web framework for Bun",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xarhang/barejs.git"
  },
  "scripts": {
    "test": "bun test",
    "benchs": "bun run benchmarks/index.ts",
    "bench": "bun run benchmarks/head-to-head.ts",
    "dev": "bun --watch example/index.ts",
    "build": "bun ./scripts/build.ts",
    "prepublishOnly": "bun run build",
    "release": "bun run build && npm version patch && npm publish --access public"
  },
  "keywords": [
    "bun",
    "framework",
    "fastest",
    "jit",
    "typebox",
    "zod",
    "barejs",
    "http-server"
  ],
  "license": "MIT",
  "peerDependencies": {
    "@sinclair/typebox": "^0.34.46",
    "zod": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "@sinclair/typebox": {
      "optional": true
    },
    "zod": {
      "optional": true
    }
  },
  "devDependencies": {
    "@sinclair/typebox": "^0.34.47",
    "@types/bun": "^1.3.5",
    "elysia": "latest",
    "hono": "latest",
    "mitata": "^1.0.34",
    "typescript": "latest",
    "zod": "^3.25.76"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ]
}
